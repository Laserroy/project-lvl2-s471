{
    "name": "lazeroy/diffgen",
    "description": "config files difference generator",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "laserX",
            "email": "deectro@gmail.com"
        }
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "funct/funct": "^1.5",
        "symfony/yaml": "^4.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^8.1"
    },
    "scripts":{
        "phpcs": "phpcs",
        "phpunit": "phpunit",
        "test": "phpunit tests"
    },
    "autoload": {
        "psr-4": {
            "DiffGenerator\\Tests\\": "tests"
        },
        "files": [
            "src/Diffgenerator.php",
            "src/DifferenceBuilder.php",
            "src/DifferenceFormatter.php"
        ]
    },
    "bin": [
        "bin/gendiff"
    ]
}
