#!/usr/bin/env php
<?php

namespace DiffGenerator;
use function DiffGenerator\show;

$autoloadPath1 = __DIR__ . '/../../../autoload.php';
$autoloadPath2 = __DIR__ . '/../vendor/autoload.php';
if (file_exists($autoloadPath1)) {
    require $autoloadPath1;
} else {
    require $autoloadPath2;
}

run();

